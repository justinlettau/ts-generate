{
  "name": "ts-generate",
  "version": "1.1.2",
  "description": "CLI to generate TypeScript interfaces from source files.",
  "keywords": [
    "typescript",
    "code generation",
    "class",
    "interface"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/justinlettau/ts-generate",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/ts-generate"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/ts-generate/issues"
  },
  "preferGlobal": true,
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "bin": {
    "ts-generate": "bin/ts-generate"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "test": "jest",
    "prebuild": "del-cli dist",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "commander": "^8.1.0",
    "fs-extra": "^10.0.0",
    "ts-morph": "^11.0.3",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.0",
    "@types/update-notifier": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^8.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
